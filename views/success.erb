<div class="content2">
<h1>Welcome! Your information is Importance to Us!</h1>
	<div class="row">
		<div class="col-md-12">

			<div class="contact-content">

				<div class="contact-row">
				<center>
				<h2>Thank You! You'll hear from us soon!</h2>
				</center>

						<form action="/contact-us" id='contact' name='email' method='post' novalidate='novalidate'>
							
							<div class="form-group">

								<div class="input-wrap">

									<input type="text" class='form-control' name='first_name' id='first_name' placeholder='First Name'>
						
								</div>
								<br>
								<div class="input-wrap">
									<input type="text" class='form-control' name='last_name' id='last_name' placeholder='Last Name'>
								</div>
								<br>
								<div class="input-wrap">
									<input type="text" class='form-control' name='mail' id='mail' placeholder='Email'>
								</div>
								<br>
								<div class="input-wrap">
									<input type="text" class='form-control' name='subject' id='subject' placeholder='Subject'>
								</div>
								<br>
								<div class="input-wrap">
									<textarea placeholder='MESSAGE' class='form-control' name="body" id="body" cols="30" rows="10"></textarea>
								</div>
								<br>
								<input class='btn btn-gray' type="submit" value='Submit' id='button'>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

